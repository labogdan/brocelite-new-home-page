class Sticky {
    constructor(selector) {
      this.$elem = $(selector);
      this.$hide = this.$elem.find(".hide");
      this.offset = {top:50};
      this.$navLinks = $(".feature_image .header .nav a");
      this.$navCartLink = $(".feature_image .header .nav a.cart-button span");

      this.$originalLogo = "{{ 'logo-2018-white.png' | asset_url }}"
      this.$hoverLogo = "{{ 'logo-2018-black.png' | asset_url }}";

      let scrollPos = $(window).scrollTop();
      if (scrollPos > 50) {
        this.positionOnScroll();
      }
    }

    positionOnScroll() {
      if ($(window).width() <= 768) {
        return;
      }

      console.log(typeof alwaysOn);

      let scrollPos = $(window).scrollTop();

      if (scrollPos >= this.offset.top) {
        this.$elem.addClass("fixed");

        this.$elem.find('.logo a img').attr("src",this.$hoverLogo);
        this.$navLinks.addClass("black_text");
        this.$navCartLink.addClass("black_text");
      } else if (document.location.href.indexOf('about') !== -1) {
          this.$elem.find('.logo a img').attr("src",this.$hoverLogo);
          this.$navLinks.addClass("black_text");
          this.$navCartLink.addClass("black_text");
      } else {
        this.$elem.removeClass("fixed");

        this.$elem.find('.logo a img').attr("src",this.$originalLogo);
        this.$navLinks.removeClass("black_text");
        this.$navCartLink.removeClass("black_text");
      }
    }
}

